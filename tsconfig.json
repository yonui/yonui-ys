{
    "compilerOptions": {
      "target": "es5",
      "module": "esnext",
      "lib": [
        "dom",
        "es2015"
      ],
      "allowJs": true,
      "jsx": "react",
      "sourceMap": true,
      "outDir": "./built",
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true
    },
    "include": [
      "./components"
    ]
  }
  